<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" packages="com.gddxit.wwis.rbac.jpa.service">

    <Appenders>
        <!--控台日志输出 -->
        <Console name="Console" target="SYSTEM_OUT" follow="true">
            <PatternLayout>
                <!--<pattern>%d %p %C{1.} [%t] %m%n</pattern> -->
                <pattern>%d{yyyy-MM-dd HH:mm:ss,SSS} [TYPE: %p | FROM: %C{1.} %M | TEXT: %m ]%n</pattern>
            </PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug" additivity="false">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>
